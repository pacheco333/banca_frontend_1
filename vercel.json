{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/angular_front/browser",
  "framework": null,
  "routes": [
    {
      "src": "/colombia-data.json",
      "dest": "/colombia-data.json"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}